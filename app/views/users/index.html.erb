<html>
<body>
  <div class="col-sm-6 blog-main">
    <div class="row">
      <div class="col-sm-9 col-sm-offset-3">
    <h8>User</h8>
    <p> All</p>
       <%= paginate @users %>
       <table class="table">
        <tr>
         <th class= "col-sm-1"> User </th>
         <th class= "col-sm-1"> Name </th>
         <th class= "col-sm-1"> Email </th>
       </tr>

       <% @users.each do |user| %>
       <tr>
         <td><%= user.id %></td>
         <td><%= user.name %></td>
         <td><%= user.email %></td>
       </tr>
       <% end %>
     </table>
   </div>
 </div>
</div>
</body>
</html>